(:name mew
       :type github
       :pkgname "kazu-yamamoto/Mew"
       :build `(("./configure" ,(concat "--with-emacs=" el-get-emacs)) "make")
       :post-init
       (progn
         (autoload 'mew "mew" nil t)
         (autoload 'mew-send "mew" nil t)
         (let*
             ((mew-path (car (el-get-load-path 'mew)))
                (mew-bin (expand-file-name "bin" mew-path)))
             (add-to-list 'exec-path mew-bin))))